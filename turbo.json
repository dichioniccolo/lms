{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "**/.env"
  ],
  "pipeline": {
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "db:studio": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".expo/**",
        ".output/**",
        ".vercel/output/**"
      ]
    },
    "db:generate": {
      "inputs": [
        "src"
      ],
      "outputs": [
        "drizzle/**"
      ],
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "format": {
      "outputs": [
        "node_modules/.cache/.prettiercache"
      ],
      "outputMode": "new-only"
    },
    "lint": {
      "dependsOn": [
        "^topo"
      ],
      "outputs": [
        "node_modules/.cache/.eslintcache"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^topo"
      ],
      "outputs": [
        "node_modules/.cache/tsbuildinfo.json"
      ]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  },
  "globalEnv": [
    "AUTH_SECRET",
    "AUTH_URL",
    "DATABASE_URL",
    "DO_ACCESS_KEY_ID",
    "DO_BUCKET_NAME",
    "DO_ENDPOINT",
    "DO_REGION",
    "DO_SECRET_ACCESS_KEY",
    "INNGEST_EVENT_KEY",
    "INNGEST_SIGNING_KEY",
    "NEXT_PUBLIC_APP_DOMAIN",
    "NEXT_PUBLIC_TEACHER_EMAILS",
    "SMTP_FROM",
    "SMTP_HOST",
    "SMTP_PORT",
    "SMTP_USERNAME",
    "SMTP_PASSWORD",
    "STRIPE_API_KEY",
    "STRIPE_WEBHOOK_SECRET"
  ]
}